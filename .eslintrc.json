{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
    "require-jsdoc": 0,
    "global-require": 0,
    "linebreak-style": 0,
    "no-undef": "off",
    "no-unused-vars": "off",
    "react/prop-types": "off",
    "quotes": ["error", "double"],
    "space-in-parens": ["error", "never"],
    "object-curly-spacing": ["error", "always"],
    "space-before-function-paren": ["error", "never"],
    "eslint linebreak-style": [0, "error", "windows"],
    "comma-spacing": [
    "error",
    {
        "after": true
    }
    ],
    "semi": ["error", "always"],
    "keyword-spacing": [
    "error",
    {
        "after": true,
        "before": true
    }
    ],
    "no-multiple-empty-lines": [
    "error",
    {
        "max": 1
    }
    ]
}
}
